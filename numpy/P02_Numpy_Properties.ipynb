   "cell_type": "code",
   "source": [
    "import numpy as np\n",
    "arr = np.array([[1,2,3],[4,5,6]])\n",
    "# print(arr)\n",
    "# arr\n",
    "print(arr.shape) # 数组的形状\n",
    "print(arr.dtype) # 数组中元素的类型\n",
    "print(arr.size)  # 数组中元素的个数\n",
    "print(arr.ndim)  # 数组的维度\n",
    "print(arr.itemsize)  # 数组中每一个元素字节大小\n",
    "print(type(arr))"
   ],
